                        owl:onProperty :hasCountry ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCountryContext ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCurrency ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasDescription ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasInitialAmount ;
                        owl:someValuesFrom xsd:double
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasSource ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasYear ;
                        owl:someValuesFrom xsd:short
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasTemporalBehavior ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange [ rdf:type rdfs:Datatype ;
                                          owl:oneOf [ rdf:type rdf:List ;
